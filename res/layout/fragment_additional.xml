            android:id="@+id/button_wolfragment_additional_percentt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="%" />
        
    </LinearLayout>

    <LinearLayout
        android:id="@+id/laylin_fragment_additional_row4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/button_wolfragment_additional_ctg"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="ctg" />
