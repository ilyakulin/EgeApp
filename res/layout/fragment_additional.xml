        <Button
            android:id="@+id/button_wolfragment_additional_ln"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="ln" />
        <Button
            android:id="@+id/button_wolfragment_additional_derivate_second"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="&#402;''(x)" />
        <Button
            android:id="@+id/button_wolfragment_additional_pi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="&#928;" />
        
    </LinearLayout>

    <LinearLayout
        android:id="@+id/laylin_fragment_additional_row3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/button_wolfragment_additional_tg"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="tg" />
        
        <Button
            android:id="@+id/button_wolfragment_additional_abs"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="|abs|" />
